{
  "compilerOptions": {
    "strict": true,
    "target": "ES2022",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": false,
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": ["src/**/*"]
}
